{
  "name": "bot-template",
  "version": "1.0.56",
  "description": "This project provides a template for creating a Discord bot using discordx and discord.js v14.",
  "license": "GPL-3.0-or-later",
  "author": "Ragnar Lothbrok <ragnarlothbrokjr@proton.me>",
  "homepage": "https://github.com/Valhalla-Development/Bot-Template#readme",
  "repository": "https://github.com/Valhalla-Development/Bot-Template.git",
  "bugs": {
    "url": "https://github.com/Valhalla-Development/Bot-Template/issues"
  },
  "type": "module",
  "main": "build/Main.js",
  "scripts": {
    "audit": "eslint . --ext .ts --format codeframe --max-warnings 0",
    "dev": "tsc-watch --onSuccess \"node ./build/Main.js\" --onFailure \"echo Compilation failed\"",
    "start": "rm -rf build && tsc && node build/Main.js"
  },
  "dependencies": {
    "@colors/colors": "^1.6.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/utilities": "^7.0.3",
    "discord.js": "^14.16.3",
    "discordx": "^11.12.1",
    "dotenv": "^16.4.5",
    "moment": "^2.30.1",
    "systeminformation": "^5.23.5"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.31.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.5.1"
}
